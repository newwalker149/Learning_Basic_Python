score = input('Enter score: ')
try:
    realscore = float(score)
    if realscore > 1.0:
        print('Bad ssore')
    elif realscore >= 0.9:
        print('A')
    elif realscore >= 0.8:
        print('B')
    elif realscore >= 0.7:
        print('C')
    elif realscore >= 0.6:
        print('D')
    elif realscore < 0.6:
        print('F')
except:
    print('Bad score')
